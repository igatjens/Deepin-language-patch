#: misc/pam-module/pam_auth.c:198
#, c-format
msgid "Verification error"
msgstr "Error de verificación"

#: misc/pam-module/pam_auth.c:201
#, c-format
msgid "Fingerprint verification unavailable, please input password"
msgstr ""
"La verificación de huella dactilar no está disponible, introduzca la "
"contraseña"

#: misc/pam-module/pam_auth.c:220 misc/pam-module/pam_auth.c:226
#, c-format
msgid "Clean your finger and try again"
msgstr "Limpie su dedo e intente nuevamente"

#: misc/pam-module/pam_auth.c:223
#, c-format
msgid "Finger not centered"
msgstr "Dedo no centrado"

#: misc/pam-module/pam_auth.c:229
#, c-format
msgid "Unclear fingerprint, please clean your finger and try again"
msgstr "Huella dactilar poco clara, limpie su dedo e intente nuevamente"

#: misc/pam-module/pam_auth.c:232
#, c-format
msgid "Finger moved too fast, please do not lift until prompted"
msgstr ""
"El dedo se movió demasiado rápido. Por favor, no lo levante hasta que se le "
"indique"

#: misc/pam-module/pam_auth.c:389
#, c-format
msgid "Verification successful"
msgstr "Verificación exitosa"

#: misc/pam-module/pam_auth.c:400
#, c-format
msgid "Verification failed, please log in with password"
msgstr "La verificación falló, inicie sesión con contraseña"

#: misc/pam-module/pam_auth.c:404
#, c-format
msgid "Verification failed, %d chances left"
msgstr "La verificación falló, quedan %d intentos"

#: misc/pam-module/pam_auth.c:406
#, c-format
msgid "Verification failed, 1 chance left"
msgstr "La verificación falló, queda 1 intento"

#: misc/pam-module/pam_auth.c:526
#, c-format
msgid "Verify your fingerprint or password"
msgstr "Verifique su huella dactilar o contraseña"

#: misc/pam-module/pam_auth.c:530
#, c-format
msgid "Password"
msgstr "Contraseña"
