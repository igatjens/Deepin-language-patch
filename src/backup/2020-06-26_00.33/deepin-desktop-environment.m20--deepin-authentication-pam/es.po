#: misc/pam-module/pam_auth.c:389
#, c-format
msgid "Verification successful"
msgstr "Verificación exitosa"

#: misc/pam-module/pam_auth.c:406
#, c-format
msgid "Verification failed, please input your password"
msgstr ""

